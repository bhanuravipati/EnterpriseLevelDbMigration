# Environment Configuration for AI Database Migration

# Groq API Configuration
GROQ_API_KEY=

GROQ_API_KEY_1= #API KEY 1
GROQ_API_KEY_2= #API KEY 2
GROQ_API_KEY_3=#API KEY 3


# Source Database (MySQL)
SOURCE_DB_TYPE=mysql
SOURCE_DB_HOST=localhost
SOURCE_DB_PORT=3306
SOURCE_DB_NAME=sakila
SOURCE_DB_USER=root
SOURCE_DB_PASSWORD=rootpass

# Target Database (PostgreSQL)
TARGET_DB_TYPE=postgresql
TARGET_DB_HOST=localhost
TARGET_DB_PORT=5432
TARGET_DB_NAME=sakila_pg
TARGET_DB_USER=postgres
TARGET_DB_PASSWORD=postgrespass

# Sandbox Database (PostgreSQL for testing)
SANDBOX_DB_HOST=localhost
SANDBOX_DB_PORT=5433
SANDBOX_DB_NAME=sandbox
SANDBOX_DB_USER=postgres
SANDBOX_DB_PASSWORD=postgrespass

# LLM Model Configuration
LLM_MODEL_COMPLEX=openai/gpt-oss-120b
LLM_MODEL_FAST=llama-3.3-70b-versatile

# Artifacts Directory
ARTIFACTS_DIR=./artifacts

# Logging
LOG_LEVEL=INFO

# LangSmith Configuration (for tracing)
# Get your API key from: https://smith.langchain.com
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=project_name_here